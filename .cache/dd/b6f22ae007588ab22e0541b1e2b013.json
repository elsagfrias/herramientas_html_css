ojects/herramientas_css_html/recetas.html"},{"name":"./img/korean_rice.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/korean_rice.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/awmuk_guk.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/awmuk_guk.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/japchae.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/japchae.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/loto.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/loto.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/오곡밥.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/오곡밥.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/consome.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/consome.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/jajangmyon.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/jajangmyon.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/cebolla.jpeg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/cebolla.jpeg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/bibimbap.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/bibimbap.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/tomanduguk.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/tomanduguk.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"./img/ramyon.jpg","dynamic":true,"resolved":"/Users/elsagomez/Projects/herramientas_css_html/img/ramyon.jpg","parent":"/Users/elsagomez/Projects/herramientas_css_html/recetas.html"},{"name":"/Users/elsagomez/Projects/herramientas_css_html/package.json","includedInParent":true,"mtime":1617137744531},{"name":"/Users/elsagomez/Projects/herramientas_css_html/.browserslistrc","includedInParent":true,"mtime":1617136380214}],"generated":{"html":"<!DOCTYPE html >\n<html lang=\"es\">\n   <head>\n      <meta charset=\"UTF-8\">\n      <link rel=\"stylesheet\" href=\"/6a6a912d0c7b55b537768da778032849.css\">\n      <title>Elsa's korean kitchen</title>\n      <link rel=\"shortcut icon\" type=\"image/jpg\" href=\"/bad9ec184b7b2e62d087b6c76aea1b07.png\">\n   </head>\n   <body>\n      <header>\n         <div>\n            <figure id=\"logo\" class=\"logo\">\n               <img src=\"/bad9ec184b7b2e62d087b6c76aea1b07.png\" alt=\"Elsa's korean kitchen\" title=\"Icono Elsa's korean kitchen\">\n               <figcaption class=\"header-figcaption\"><a href=\"/eacf331f0ffc35d4b482f1d15a887d3b.html\">Elsa's korean kitchen</a></figcaption>\n            </figure>\n         </div>\n         <div>\n            <nav class=\"header-menu\">\n               <ul>\n                  <li><a href=\"/ff0fefcb7ddf0bbf66ee032b472157ab.html\"><strong>Gastronomía coreana</strong></a></li>\n                  <li class=\"header-li\"><a href=\"/7fc595b78f152746327a593e029f13cc.html\"><strong>Dónde comer</strong></a></li>\n                  <li class=\"header-li\"><a href=\"/2b0f5330b50da81bc31cf65f5a38814a.html\"><strong><em>Recetas</em></strong></a></li>\n                  <li class=\"header-li\"><a href=\"/fb8a140d6f076647256a4ea161f4c280.html\"><strong>Sobre nosotros</strong></a></li>\n                  <li class=\"header-li\"><a href=\"/fb755ef1b4894e6f1a3e32d8571b0c79.html\"><strong>Referencias</strong></a></li>\n               </ul>\n            </nav>\n         </div>\n      </header>\n      <main class=\"no-index-main\">\n         <h1>Recetas</h1>\n         <div class=\"recetas-container\">\n            <div id=\"myBtnContainer\">\n               <button class=\"btn active\" onclick=\"l('all')\"> Todas</button>\n               <button class=\"btn\" onclick=\"l('arroz')\"> Arroces</button>\n               <button class=\"btn\" onclick=\"l('sopa')\"> Sopas</button>\n               <button class=\"btn\" onclick=\"l('encurtido')\"> Encurtidos</button>\n               <button class=\"btn\" onclick=\"l('fideo')\"> Fideos</button>\n            </div>\n            <div>\n               <div class=\"filterDiv encurtido show\">\n                  <figure class=\"receta\">\n                     <img src=\"/be17031b712ad4f85bed949b2242f376.JPG\" alt=\"Imagen de un bote con kimchi\" title=\"Kimchi de col clásico (배추김치)\">\n                     <figcaption>\n                        <strong>Kimchi de col clásico (배추김치)</strong>\n                        <br>\n                        <p>El kimchi es el plato nacional de Corea y se sirve en todas las comidas, así que este kimchi de col clásico es un buen comienzo. El kimchi casero lleva tiempo, pero una vez hecho se conserva en el frigorífico meses y el sabor es mejor.</p>\n                        <a class=\"button-link\" href=\"/accc3871b88c1b1a97be22c795feb5d8.html\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv arroz show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/877e72360cabbef24084a1d7f4488c0f.jpg\" alt=\"Imagen de un bol con arroz hervido\" title=\"Arroz de grano corto (밥)\"></a>\n                     <figcaption>\n                        <strong>Arroz de grano corto (밥)</strong>\n                        <br>\n                        <p>Escurre y enjuaga el arroz 3 veces en agua fría. Luego pásalo a un cuenco, cubre con agua fría y deja que se ablande 30 minutos. Cuela el arroz a una oola de fondo grueso con una tapa que cierre bien. Vierte 500ml de agua, tapa la olla...</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv sopa show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/36270e77ded3ad6bee905b680d94bdaf.jpg\" alt=\"Imagen de un cuenco con sopa de pasteles de pescado\" title=\"Sopa de pasteles de pescado (어묵국)\"></a>\n                     <figcaption>\n                        <strong>Sopa de pasteles de pescado (어묵국)</strong>\n                        <br>\n                        <p>Uno de los mejores recuerdos que me trae este plato ocurrió en un bar que visité la primera vez que fui a Corea. Allí lo servían como comida de acompañamiento a las bebidas alcohólicas para aguantar sobrios más tiempo. No estoy muy segura...</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv fideo show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/ebef7e57fbf85078a2ebe3f99be8ce84.jpg\" alt=\"Imagen del plato coreano de fideos celofán\" title=\"Fideos de celofán de sésamo con ternera (잡채)\"></a>\n                     <figcaption>\n                        <strong>Fideos de celofán de sésamo con ternera (잡채)</strong>\n                        <br>\n                        <p>Posiblemente uno de los platos de fideos más famosos de Corea, fácil de preparar, sano y repleto de sabor gracias a la salsa de soja y los fideos bañados en sésamo. Tradicionalmente se degusta en fiestas y ocasiones especiales.</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv encurtido show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/65b6757bff43289a756c0daa2b5d9975.jpg\" alt=\"Imagen del platillo de raíz de loto sazonada\" title=\"Raíz de loto sazonada (연근 조림)\"></a>\n                     <figcaption>\n                        <strong>Raíz de loto sazonada (연근 조림)</strong>\n                        <br>\n                        <p>Lo maravilloso de las guarniciones coreanas son sus variadas texturas. En esta receta, la raíz de loto se cocina a fuego lento en salsa de soja hasta obtener una consistencia de sirope, lo que aporta una textura...</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv arroz show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/51463bed8ab3827b222bb46d1b93b8ba.jpg\" alt=\"Imagen de un cuenco con arroz de cinco granos\" title=\"Arroz de cinco granos (오곡밥)\"></a>\n                     <figcaption>\n                        <strong>Arroz de cinco granos (오곡밥)</strong>\n                        <br>\n                        <p>Si dispones de una arrocera, mezcla todo y cocina de acuerdo a las instrucciones que te indicaré más adelante. Si no, escurre las judías, ponlas en una olla y cúbrelas con agua. Cuando hiervan, retira la espuma...</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv sopa show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/eb46982675ddf4bd0d11bba40a348951.jpg\" alt=\"Imagen del plato coreano de consomé con almejas\" title=\"Consomé de almejas (조개탕)\"></a>\n                     <figcaption>\n                        <strong>Consomé de almejas (조개탕)</strong>\n                        <br>\n                        <p>Este delicioso y reconfortante consomé se prepara en un momento. La pasta de soja doenjang aporta intensidad al sabor, pero no la uses si lo que quieres es un consomé suave. Primero deberás limpiar las almejas...</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv fideo show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/eb5c63bff720bde01011e06efc890000.jpg\" alt=\"Imagen del plato coreano de fideos con judías negras\" title=\"Fideos con judías negras (짜장면)\"></a>\n                     <figcaption>\n                        <strong>Fideos con judías negras (짜장면)</strong>\n                        <br>\n                        <p>Siendo estricta, este no es un plato tradicional coreano, sino una reinvención del famoso  original chino, pero es tan extremadamente popular en Corea que debía incluirlo. Puedes usar las verduras y carne que quieras.</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv encurtido show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/ed75b924a25102510e82da3ea309b1b8.jpeg\" alt=\"Imagen del platillo de cebolla encurtida\" title=\"Cebollas encurtidas (양파절임)\"></a>\n                     <figcaption>\n                        <strong>Cebollas encurtidas (양파절임)</strong>\n                        <br>\n                        <p>Este encurtido de cebolla se hace en un momento y es muy sencillo si cuentas con todos los ingredientes. En Corea se sirve siempre en las barbacoas y es obligado con las costillas de ternera a la parrilla. </p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv arroz show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/d37a19230b9c4399074201ad81375ba2.jpg\" alt=\"Imagen del plato coreano de arroz mixto con verduras y ternera\" title=\"Arroz mixto con verduras y ternera (비빔밥)\"></a>\n                     <figcaption>\n                        <strong>Arroz mixto con verduras y ternera (비빔밥)</strong>\n                        <br>\n                        <p>Cuesta no enamorarse del bibimbap. Una base de arroz blanco perlado adornado con el colorado anillo que forman las verduras y la ternera, y en el centro una relumbrante estrella, la yema amarilla del huevo.</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv sopa show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/c32eb60bbc324c864cdc3ab9af5bcb7d.jpg\" alt=\"Imagen del plato coreano de sopa con empanadas de pollo\" title=\"MSopa de dumplings de pollo (떡만두국)\"></a>\n                     <figcaption>\n                        <strong>Sopa de dumplings de pollo (떡만두국)</strong>\n                        <br>\n                        <p>Los dumplings coreanos difieren de la mayoría de dumplings asiáticos ya que, además de carne, incorporan pollo y tofú. El kimchi y los fideos celofán también son habituales. Los puedes comprar congelados, pero si dispones de tiempo, los caseros merecen la pena. </p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n               <div class=\"filterDiv fideo show\">\n                  <figure class=\"receta\">\n                     <a href=\"#\"><img src=\"/41eba7ce033d167055a9a4b87af0e657.jpg\" alt=\"Imagen del plato coreano de fideos instantáneos\" title=\"Ramen de pollo (라면)\"></a>\n                     <figcaption>\n                        <strong>Ramen de pollo (라면)</strong>\n                        <br>\n                        <p>Esta no es una receta en el sentido estricto de la palabra, pero en mi defensa diré que el ramen instantáneo es tan coreano como un tarro de kimchi en fermentación, así que debía incluirla. Jóvenes y mayores, con resaca o poco tiempo, disfrutan de este ramen.</p>\n                        <a class=\"button-link\" href=\"#\">Receta completa</a>\n                     </figcaption>\n                  </figure>\n               </div>\n            </div>\n         </div>\n      </main>\n      <footer>\n         <div class=\"footer-nav\">\n            <div class=\"footer-div\">\n               <a class=\"footer-link\" href=\"/eacf331f0ffc35d4b482f1d15a887d3b.html#logo\">Elsa's korean kitchen</a>\n            </div>\n            <div>\n               <nav>\n                  <ul class=\"footer-menu\">\n                     <li><a href=\"/ff0fefcb7ddf0bbf66ee032b472157ab.html\">Gastronomía coreana</a></li>\n                     <li><a href=\"/7fc595b78f152746327a593e029f13cc.html\">Dónde comer</a></li>\n                     <li><a href=\"/2b0f5330b50da81bc31cf65f5a38814a.html\">Recetas</a></li>\n                     <li><a href=\"/fb8a140d6f076647256a4ea161f4c280.html\">Sobre nosotros</a></li>\n                     <li><a href=\"/fb755ef1b4894e6f1a3e32d8571b0c79.html\">Referencias</a></li>\n                  </ul>\n               </nav>\n            </div>\n         </div>\n         <div class=\"author-container\">\n            <div class=\"author-name\">\n               <p>&copy; 2021 Elsa Gómez Frias</p>\n            </div>\n         </div>\n      </footer>\n      <script>function filterSelection(c) {\n  var x, i;\n  x = document.getElementsByClassName(\"filterDiv\");\n  if (c == \"all\") c = \"\";\n\n  for (i = 0; i < x.length; i++) {\n    eliminarClase(x[i], \"show\");\n    if (x[i].className.indexOf(c) > -1) nuevaClase(x[i], \"show\");\n  }\n}\n\nfunction nuevaClase(element, name) {\n  var i, arr1, arr2;\n  arr1 = element.className.split(\" \");\n  arr2 = name.split(\" \");\n\n  for (i = 0; i < arr2.length; i++) {\n    if (arr1.indexOf(arr2[i]) == -1) {\n      element.className += \" \" + arr2[i];\n    }\n  }\n}\n\nfunction eliminarClase(element, name) {\n  var i, arr1, arr2;\n  arr1 = element.className.split(\" \");\n  arr2 = name.split(\" \");\n\n  for (i = 0; i < arr2.length; i++) {\n    while (arr1.indexOf(arr2[i]) > -1) {\n      arr1.splice(arr1.indexOf(arr2[i]), 1);\n    }\n  }\n\n  element.className = arr1.join(\" \");\n}\n\nfilterSelection(\"all\");</script>\n   </body>\n</html>"},"sourceMaps":null,"error":null,"hash":"66608666b462605cce7ab50b25defbae","cacheData":{"env":{}}}